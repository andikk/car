{
  "name": "car",
  "version": "1.0.0",
  "private": true,
  "description": "car",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cheerio": "^0.6.3",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-imagemin": "^3.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "4.0.x",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.4",
    "gulp-uglify": "^1.5.4",
    "gulp-watch": "^4.3.11",
    "pump": "^2.0.0"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"_source/css/**/*.scss\" --syntax scss",
